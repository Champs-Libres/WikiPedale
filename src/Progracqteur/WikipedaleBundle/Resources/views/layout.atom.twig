<?xml version="1.0" encoding="utf-8"?>
{#
  Usage :  
- the following block MUST BE OVERRIDEN:
     - id
     - linkself
     - entries

- it is recommanded to inherit the following block: 
     - title
     - subtitle
     

#}
<feed xmlns="http://www.w3.org/2005/Atom" 
      xmlns:georss="http://www.georss.org/georss">
 
 <title type="html">{%block title%}Titre d'exemple{% endblock %} | Wikipedale</title>
 <subtitle type="html">{% block subtitle %}Titre secondaire {% endblock %}</subtitle>
 <link href="{% block link %}http://wikipedale.org{% endblock %}" rel="alternate" type="text/html" />
 <link href="{% block linkself %}{% endblock %}" rel="self" type="application/atom+xml" />
 <updated>{% block updated %}{{ "now"|date("Y-m-d\\TH:i:sP") }}{% endblock %}</updated>
 <author>
   <name>Wikipedale Admin</name>
   <email>admin@wikipedale.org</email>
 </author>
 <id>{% block id %}{% endblock %}</id>
 
 {% block entries %}
 
 <entry>
   <title type="html">Des robots propuls√©s par Atom deviennent fous</title>
   <link href="http://example.org/2003/12/13/atom03"/>
   <id>urn:uuid:1225c695-cfb8-4ebb-aaaa-80da344efa6a</id>
   <updated>2010-04-01T18:30:02Z</updated>
   <content type="html">Poisson d'avril !</content>
 </entry>
 
 {% endblock %}
 
</feed>
