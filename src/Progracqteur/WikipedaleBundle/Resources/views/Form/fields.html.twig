{# this form is used when rendering the form which change notification frequency. #}
{% block notification_frequency_widget %}



    <ul {{ block('widget_container_attributes') }} class="notification_frequency">
    {% for child in form %}
        <li class="notification_frequency_element">{{form_widget(child)}} {{form_label(child)}}</li>
    {% endfor %}
    </ul>



{% endblock %}
