    <img src="{{ asset('img/logos/logo_uello_300.png') }}" />
    <h1>
        <span style="color:#008000;">{{ 'accueil.assoc'|trans({}, 'messages') }}</span>
        {{ 'accueil.accroche'|trans({}, 'messages') }}
